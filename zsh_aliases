# Misc
alias tma='tmux attach-session -t'
alias tm='tmux new -A -s pat'
alias cl='clear'
alias ..='cd ..'
alias done='espeak done && hterm-notify done; notify-send done'
alias porthog='netstat -tulpn | grep'

# Git
alias gs='git status'
alias gp='git pull'
alias gc='git commit'
alias gpr='pr'
alias ga='git add --all .'
alias gl='git log'
alias gd='git diff'
alias gcm='git checkout main'
alias gb='git branch'
alias gca='git commit --amend --no-edit'
